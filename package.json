{
  "name": "ndoojs",
  "version": "2.0.0b1",
  "description": "ndoojs",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndoojs/ndoojs.git"
  },
  "scripts": {
    "postinstall": "bower install",
    "test": "karma run",
    "start-test": "karma start test/conf/unit.conf.js --browsers=PhantomJS",
    "start-test-dev": "karma start test/conf/unit.conf.js --browsers=Chrome",
    "build": "export NODE_ENV=production && webpack",
    "dev-server": "webpack-dashboard -- webpack-dev-server --hot --inline --host=0.0.0.0",
    "build-dist": "export NODE_ENV=production && export TARGET=dist && webpack",
    "build-node": "export NODE_ENV=production && export TARGET=node && webpack",
    "doc": "jsdoc -c jsdoc.conf js/ndoo.js readme.md",
    "cpjs": "cp -fr js/*.js docs/example/js/",
    "old-watch": "acmd watch",
    "old-build": "acmd run",
    "old-build-ls": "find _source -type f -name *.ls | acmd run -o",
    "old-dist": "cat js/ndoo_prep.js js/ndoo_lib_depend.js js/ndoo_lib.js js/ndoo.js js/ndoo_block.js js/ndoo_service.js > dist/ndoo_all.js && cat js/ndoo_prep.js js/ndoo_lib_depend.js js/ndoo.js js/ndoo_block.js js/ndoo_service.js > dist/ndoo_slim.js && uglifyjs -rco dist/ndoo_all.min.js dist/ndoo_all.js && uglifyjs -rco dist/ndoo_slim.min.js dist/ndoo_slim.js"
  },
  "author": "chenglifu",
  "devDependencies": {
    "autocommand-cli": "lccf/autocommand-cli#latest",
    "bower": "^1.7.9",
    "coffee-script": "^1.10.0",
    "html-webpack-plugin": "^2.28.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jasmine-core": "^2.3.4",
    "jsdoc": "^3.4.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coffee-preprocessor": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "markdown-js": "0.0.3",
    "node-sass": "^3.8.0",
    "phantomjs": "^2.1.7+deprecated",
    "phantomjs-polyfill": "^0.0.2",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.9.4",
    "typescript": "^2.0.3",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.13.2",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "underscore": "^1.8.3"
  }
}
